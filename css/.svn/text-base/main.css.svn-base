body{
	font-family: 'Open Sans', sans-serif;
    font-size: 12px;
    color: #666;
    margin: 0;
    line-height: 20px;
    overflow-x: hidden;
    font-family: 'Open Sans', sans-serif;
    font-size: 12px;
    color: #666;
    background: #2E363F;
    width:100%;
    height:100%;
}
a:-webkit-any-link {
    color: -webkit-link;
    text-decoration: none;
    cursor: auto;
}
* {
	padding:0;
	margin:0;
    outline: none !important;
    -moz-outline: none !important;
}
h1, h2, h3, h4, h5, h6 {
    font-family: inherit;
    font-weight: bold;
    color: inherit;
    text-rendering: optimizelegibility;
}
h1, h2, h3 {
    line-height: 40px;
}
h1 {
    font-size: 38.5px;
}
.sidebar{
	position:absolute;
	top:0px;
	left:0px;
	width:240px;
	height:100%;
	z-index:22;
}
.logo-box{
	position:relative;
	width:100%;
	height:77px;
	background: #1f262d;
}
.logo-box h1{
	position:absolute;
	width:100%;
	height:77px;
	background:url(../img/logo.png) center center no-repeat;
}
.logo-box h1 a{
	display:block;
	width:100%;
	height:100%;
	line-height:77px;
	color:#fff;
	text-align:center;
	font-size: 38.5px;
}
.sidebar-items{
	display:block;
	list-style: none;
    margin: 0px;
    padding: 0;
    position: absolute;
    width: 240px;
    border-bottom: 1px solid #37414b;
}
.sidebar-items> li{
	display: block;
    position: relative;
    border-top: 1px solid #37414b;
    border-bottom: 1px solid #1f262d;
}
.sidebar-items > li> a{
	position:relative;
	padding: 10px 0 10px 8px;
    display: block;
    color: #939da8;
    line-height: 20px;
}
.sidebar-items> li:hover, .sidebar-items >li.active{
    background: #27a9e3;
    border-bottom: 1px solid #27a9e3;
    border-top: 1px solid #27a9e3;
}
.sidebar-items> li.active{
	border-bottom: 1px solid #1f262d;
}
.sidebar-items > li.active> a, .sidebar-items> li:hover> a{
	color: #fff;
}
.sidebar-items> li.active > a{
	background: url("../img/menu-active.png") no-repeat scroll right center transparent !important;
}
.sidebar-items > li > a > i {
    opacity: .5;
    margin-top: 2px;
    margin-right: 2px;
}
.sidebar-items > li > a > .label {
	position:absolute;
   	right:20px;
   	border-radius: 0px;
    padding: 3px 5px 2px;
    background: #F66;
    display: inline-block;
    font-size: 11.844px;
    font-weight: bold;
    line-height: 14px;
    color: #fff;
    text-shadow: 0 -1px 0 rgba(0,0,0,0.25);
    white-space: nowrap;
    vertical-align: baseline;
}
.sidebar-items > li > ul{
	display: none;
    margin: 0;
    padding: 0;
	background-color: #1e242b;
}
.sidebar-items > li.open >ul{
	display:block;
}
.sidebar-items > li > ul > li{
	line-height: 20px;
}
.sidebar-items > li > ul > li a{
	padding: 10px 0 10px 25px;
    display: block;
    color: #939da8;
}
.sidebar-items > li.active >ul >li:last-child{
	border-bottom: 1px solid #27a9e3;
}
.sidebar-items > li > ul > li a:hover{
	color: #fff;
    background-color: #28b779;
}
.content{
	position:relative;
	margin-left:240px;
	height:100%;
	background:#eeeeee;
	z-index:10;
	overflow:hidden;
}
.header{
	position:relative;
	background-color: #1f262d;
	height:40px;
}
.header .nav{
	position: relative;
    left: 0;
    display: block;
    float: left;
    margin: 0;
    padding: 0;
    list-style: none;
    border-right: 1px solid #2e363f;
    border-left: 1px solid #000;
}
.header .nav li{
	line-height: 20px;
	float: left;
    list-style-type: none;
    margin: 0;
    position: relative;
    padding: 0;
    border-left: 1px solid #2e363f;
    border-right: 1px solid #000;
}
.nav>li>a {
    float: none;
    text-decoration: none;
    color: #999;
    text-shadow: 0 -1px 0 rgba(0,0,0,0.25);
    padding: 10px 10px;
    display: block;
    font-size: 11px;
}
.nav>li>a:hover, .nav>li>a.active{
	color: #ffffff;
    background: #000;
}
.Hui-userbar{
	list-style-type: none;
	position: absolute;
    top: 0px;
    right: 20px;
    -webkit-font-smoothing: antialiased;
    font-family: "Microsoft Yahei","Hiragino Sans GB","Helvetica Neue",Helvetica,tahoma,arial,"WenQuanYi Micro Hei",Verdana,sans-serif,"\5B8B\4F53";
}
.Hui-userbar > li {
    float: left;
    display: inline-block;
    position: relative;
    line-height: 44px;
    color:#fff;
}
.Hui-userbar > li > a {
    display: inline-block;
    padding: 0 10px;
    height: 44px;
    line-height: 44px;
    color:#fff;
}
.Hui-userbar > li > a:hover, Hui-userbar > li.current > a {
    text-decoration: none;
    background-color: rgba(255,255,255,0.2);
    color: #fff;
}
.dropDown.hover .dropDown_A, .dropDown.open .dropDown_A {
    text-decoration: none;
    background-color: rgba(255,255,255,0.2);
}
.Hui-iconfont {
    font-family: "Hui-iconfont" !important;
    font-style: normal;
    -webkit-font-smoothing: antialiased;
    -webkit-text-stroke-width: 0.2px;
    -moz-osx-font-smoothing: grayscale;
}
.dropDown_A i {
    transition-duration: .3s;
    transition-property: all;
}
.content-header{
	position:relative;
	width:100%;
	z-index:20;
}
.breadcrumb{
	background-color: #fff;
    border-bottom: 1px solid #e3ebed;
}
.breadcrumb a {
    padding: 8px 20px 8px 10px;
    display: inline-block;
    background-image: url('../img/breadcrumb.png');
    background-position: center right;
    background-repeat: no-repeat;
    font-size: 11px;
    color: #666666;
}
.breadcrumb a:last-child {
    background-image: none;
    font-weight: bold;
    color: #444444;
}
.breadcrumb a i {
    margin-right: 5px;
    opacity: .6;
}
.breadcrumb a.current {
    font-weight: bold;
    color: #444444;
}
.content-box{
	position:relative;
	padding:0;
	margin:0;
	width:100%;
	height:auto;
	font-size: 14px;
    color: #333;
    overflow:hidden;
}
.Hui-aside {
    position: absolute;
    top: 0px;
    bottom: 0;
    left: 0;
    padding-top: 10px;
    width: 150px;
    z-index: 99;
    overflow: auto;
    background-color: rgba(238,238,238,0.98);
    border-right: 1px solid #e5e5e5;
    
     -moz-transition: all 0.2s ease 0s;
    -webkit-transition: all 0.2s ease 0s;
    -ms-transition: all 0.2s ease 0s;
    -o-transition: all 0.2s ease 0s;
    transition: all 0.2s ease 0s;
}
.Hui-aside._hide{
	left: -150px;
}
.menu_dropdown > a{
	display: block;
    line-height: 35px;
    padding-left: 15px;
    cursor: pointer;
    position: relative;
    border-bottom: 1px solid #e5e5e5;
    font-weight: normal;
    color: #333;
}
.menu_dropdown > a:hover{
	color: #148cf1;
}
.dislpayArrow {
    position: absolute;
    top: 0;
    bottom: 0;
    left: 151px;
    width: 0px;
    height: 100%;
    z-index: 10;
     -moz-transition: all 0.2s ease 0s;
    -webkit-transition: all 0.2s ease 0s;
    -ms-transition: all 0.2s ease 0s;
    -o-transition: all 0.2s ease 0s;
    transition: all 0.2s ease 0s;
}
.dislpayArrow._hide{
	left: 0px;
}
.dislpayArrow a {
    position: absolute;
    display: block;
    width: 17px;
    height: 61px;
    top: 50%;
    margin-top: -30px;
    outline: none;
    color: #333;
}
.dislpayArrow a {
    background: url(../img/icon_arrow.png) no-repeat 0 0;
}
.dislpayArrow a.open {
    background-position: 0 -61px;
}
.dislpayArrow a:hover {
    text-decoration: none;
    background-position: right 0;
}
.dislpayArrow a.open:hover{
    background-position: right -61px;
}
.Hui-article-box, .data-chart{
	position: absolute;
    top: 0px;
    right: 0;
    bottom: 0;
    left: 151px;
    overflow: hidden;
    z-index: 1;
    background-color: #fff;
    -moz-transition: all 0.2s ease 0s;
    -webkit-transition: all 0.2s ease 0s;
    -ms-transition: all 0.2s ease 0s;
    -o-transition: all 0.2s ease 0s;
    transition: all 0.2s ease 0s;
}
.Hui-article-box.full_show, .data-chart.full_show{
	left: 1px;
}
/* start of mask */
.mask-layer{
    background: rgba(0, 0, 0, 0.13);
    position: absolute;
    top: 0;
    z-index: 99999;
    height: 100%;
    width: 100%;
    opacity:0;
    display:none;
    -moz-transition: opacity 0.2s ease 0s;
    -webkit-transition: opacity 0.2s ease 0s;
    -ms-transition: opacity 0.2s ease 0s;
    -o-transition: opacity 0.2s ease 0s;
    transition: opacity 0.2s ease 0s;
}
.main_inner{
	position:absolute;
	width:80px;
	height:80px;
	left:50%;
	top:50%;
	margin-left:-40px;
	margin-top:-40px;
}
.mask-div{
    position: relative;
    width: 80px;
    height: 80px;
    left: 50%;
    top: 40%;
    margin-top: -40px;
}

@-webkit-keyframes line-spin-fade-loader {
    50% {
        opacity: 0.3; }

    100% {
        opacity: 1; } }

@keyframes line-spin-fade-loader {
    50% {
        opacity: 0.3; }

    100% {
        opacity: 1; } }

.line-spin-fade-loader {
    position: relative; }

.line-spin-fade-loader > div:nth-child(1) {
    top: 20px;
    left: 0;
    -webkit-animation: line-spin-fade-loader 1.2s 0.12s infinite ease-in-out;
    animation: line-spin-fade-loader 1.2s 0.12s infinite ease-in-out; }
.line-spin-fade-loader > div:nth-child(2) {
    top: 13.63636px;
    left: 13.63636px;
    -webkit-transform: rotate(-45deg);
    -ms-transform: rotate(-45deg);
    transform: rotate(-45deg);
    -webkit-animation: line-spin-fade-loader 1.2s 0.24s infinite ease-in-out;
    animation: line-spin-fade-loader 1.2s 0.24s infinite ease-in-out; }
.line-spin-fade-loader > div:nth-child(3) {
    top: 0;
    left: 20px;
    -webkit-transform: rotate(90deg);
    -ms-transform: rotate(90deg);
    transform: rotate(90deg);
    -webkit-animation: line-spin-fade-loader 1.2s 0.36s infinite ease-in-out;
    animation: line-spin-fade-loader 1.2s 0.36s infinite ease-in-out; }
.line-spin-fade-loader > div:nth-child(4) {
    top: -13.63636px;
    left: 13.63636px;
    -webkit-transform: rotate(45deg);
    -ms-transform: rotate(45deg);
    transform: rotate(45deg);
    -webkit-animation: line-spin-fade-loader 1.2s 0.48s infinite ease-in-out;
    animation: line-spin-fade-loader 1.2s 0.48s infinite ease-in-out; }
.line-spin-fade-loader > div:nth-child(5) {
    top: -20px;
    left: 0;
    -webkit-animation: line-spin-fade-loader 1.2s 0.6s infinite ease-in-out;
    animation: line-spin-fade-loader 1.2s 0.6s infinite ease-in-out; }
.line-spin-fade-loader > div:nth-child(6) {
    top: -13.63636px;
    left: -13.63636px;
    -webkit-transform: rotate(-45deg);
    -ms-transform: rotate(-45deg);
    transform: rotate(-45deg);
    -webkit-animation: line-spin-fade-loader 1.2s 0.72s infinite ease-in-out;
    animation: line-spin-fade-loader 1.2s 0.72s infinite ease-in-out; }
.line-spin-fade-loader > div:nth-child(7) {
    top: 0;
    left: -20px;
    -webkit-transform: rotate(90deg);
    -ms-transform: rotate(90deg);
    transform: rotate(90deg);
    -webkit-animation: line-spin-fade-loader 1.2s 0.84s infinite ease-in-out;
    animation: line-spin-fade-loader 1.2s 0.84s infinite ease-in-out; }
.line-spin-fade-loader > div:nth-child(8) {
    top: 13.63636px;
    left: -13.63636px;
    -webkit-transform: rotate(45deg);
    -ms-transform: rotate(45deg);
    transform: rotate(45deg);
    -webkit-animation: line-spin-fade-loader 1.2s 0.96s infinite ease-in-out;
    animation: line-spin-fade-loader 1.2s 0.96s infinite ease-in-out; }
.line-spin-fade-loader > div {
    background-color: #fff;
    width: 4px;
    height: 35px;
    border-radius: 2px;
    margin: 2px;
    -webkit-animation-fill-mode: both;
    animation-fill-mode: both;
    position: absolute;
    width: 5px;
    height: 15px; }

/* end of mask */

/* the norm */
#gritter-notice-wrapper {
	position:fixed;
	display:none;
	top:50px;
	right:10px;
	width:301px;
	z-index:989;
}
#gritter-notice-wrapper.top-left {
    left: 20px;
    right: auto;
}
#gritter-notice-wrapper.bottom-right {
    top: auto;
    left: auto;
    bottom: 20px;
    right: 20px;
}
#gritter-notice-wrapper.bottom-left {
    top: auto;
    right: auto;
    bottom: 20px;
    left: 20px;
}
.gritter-item-wrapper {
	position:relative;
	margin:0 0 10px 0;
}

.gritter-top, .gritter-bottom {
	height: 0;
}

.gritter-item {
	display:block;
	background: #f74d4d; /* Old browsers */
	color:#2b2b2b; box-shadow:3px 3px 20px #000;
	padding:7px 10px 10px;
	font-size: 11px; color:#fff;
	font-family:verdana;
}
.hover .gritter-item {
}
.gritter-item p {
	padding:0;
	margin:0;
	word-wrap:break-word;
	font-size: 10px;
	line-height: 14px;
}
.gritter-close {
	position:absolute;
	top:-11px;
	right:-11px;
	background:url(../img/gritter.png) no-repeat left top;
	cursor:pointer;
	width:30px;
	height:30px;
}
.gritter-title {
	font-size:12px;
	font-weight:bold;
	padding:0 0 7px 0;
	display:block;
}
.gritter-image {
	width:32px;
	height:32px;
	float:left;
	margin: 5px;
	margin-top:10px;
}
.gritter-with-image,
.gritter-without-image {
	padding:0;
}
.gritter-with-image {
	width:220px;
	float:right;
}
/* for the light (white) version of the gritter notice */
.gritter-light .gritter-item,
.gritter-light .gritter-bottom,
.gritter-light .gritter-top,
.gritter-light .gritter-close {
    background-image: url(../img/gritter-light.png);
    color: #222;
}
.gritter-light .gritter-title {
    text-shadow: none;
}
